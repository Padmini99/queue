// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    queue<int>q;
    q.push(3);
    q.push(4);
    q.push(5);
    cout<<"top :"<<q.front()<<endl;
    q.pop();
    cout<<"top :"<<q.front()<<endl;
    cout<<"size: "<<q.size()<<endl;
    if(q.empty()){
        cout<<"queue empty"<<endl;
    }
    else{
        cout<<"queue not empty"<<endl;
    }
    return 0;
}
